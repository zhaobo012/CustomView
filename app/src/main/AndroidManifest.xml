<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhaobo.progressviews">
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.PullToListViewActivity" />
        <activity android:name=".ui.BezierActivity" />
        <activity android:name=".ui.TouTiaoActivity" />
        <activity android:name=".ui.ZoomImageViewActivity" />
        <activity android:name=".ui.SlideViewActivity" />
        <activity android:name=".ui.PolyActivity" />
        <activity android:name=".ui.RemoteControlActivity" />
        <activity android:name=".ui.Rotate3DAnimationActivity" />
        <activity android:name=".ui.CustomViewgroupActivity" />
        <activity android:name=".ui.PermisionActivity"/>
        <activity android:name=".ui.Custom3DActivity"/>
    </application>

</manifest>